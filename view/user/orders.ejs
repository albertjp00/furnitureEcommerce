   // Event listener for quantity changes
// Event listener for quantity changes


// Get all input fields with class "quantity-input"
const quantityInputs = document.querySelectorAll('.quantity-input');

// Loop through each input field




quantityInputs.forEach(function(inputField) {
    // Extract product index from input field ID
    const productId = inputField.dataset.productid;

    inputField.addEventListener('input', function () {
        // Log a message to verify that the event listener is triggered
        console.log('Quantity input changed');

        // You can also log the current value of the input field
        console.log('Current value:', this.value);

        // Prepare the data to be sent to the server
        const data = {
            productId: productId,
            quantity: this.value
        };

        // Make a fetch request to update the quantity on the server
        fetch('/user/updateQuantity', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(data)
        })
        .then(response => {
            if (!response.ok) {
                throw new Error('Failed to update quantity');
            }
            return response.json();
        })
        .then(data => {
            if (data.showAlert) { // Corrected to data.showAlert
               alert(data.error);
            }
            console.log('Quantity updated successfully', data);
            // Optionally, you can update the UI to reflect the new quantity
        })
        .catch(error => {
            console.error('Error updating quantity:', error);
        });
    });
});









