        <script>
            function validatePassword() {
    const password1 = document.getElementById('addaddress-Name').value;
    const password2 = document.getElementById('addaddress-phone').value;
    const mismatchMessage = document.getElementById('passwordMismatchMessage');
    const passwordWrongModal = new bootstrap.Modal(document.getElementById('passwordWrongModal')); // Initialize the modal

    if (password1 !== password2) {
        mismatchMessage.textContent = "Password not matching"; // Display error message
        passwordWrongModal.show(); // Show the modal
        return false; // Prevent form submission
    } else {
        mismatchMessage.textContent = ""; // Clear error message if passwords match
    }
    return true; // Allow form submission
}



    // Add event listener to the "Remove" button
    document.querySelectorAll('.remove-address-btn').forEach(button => {
        button.addEventListener('click', function(event) {
            event.preventDefault(); // Prevent the default action of the link

            // Show the modal
            var modal = new bootstrap.Modal(document.getElementById('passwordWrongModal'));
            modal.show();

            
        });
    });



    
    document.addEventListener('DOMContentLoaded', function () {
        var form = document.getElementById('editProfileForm');

        form.addEventListener('submit', function (event) {
            // Prevent the form from submitting by default
            event.preventDefault();

            // Remove whitespace from the beginning and end of input fields
            var nameInput = document.getElementById('addaddress-Name');
            nameInput.value = nameInput.value.trim();
            var phoneInput = document.getElementById('addaddress-phone');
            phoneInput.value = phoneInput.value.trim();

            // Validate name field
            if (nameInput.value === '') {
                nameInput.setCustomValidity('Please enter your name.');
            } else {
                nameInput.setCustomValidity('');
            }

            // Validate phone number
            var phoneNumber = phoneInput.value;
            if (!/^[1-9][0-9]{9}$/.test(phoneNumber)) {
                phoneInput.setCustomValidity('Please enter a valid 10-digit phone number not starting with 0.');
            } else {
                phoneInput.setCustomValidity('');
            }

            // Check if form is valid
            if (form.checkValidity()) {
                // If valid, submit the form
                form.submit();
            } else {
                // If not valid, display validation messages
                form.classList.add('was-validated');
            }
        }, false);
    });

    // for the delete modal to appear 

    
    const removeAddressBtns = document.querySelectorAll('.remove-address-btn');
    removeAddressBtns.forEach(btn => {
        btn.addEventListener('click', () => {
            const targetModalId = btn.getAttribute('data-bs-target');
            const targetModal = document.querySelector(targetModalId);
            if (targetModal) {
                new bootstrap.Modal(targetModal).show();
            }
        });
    });

    
</script>

